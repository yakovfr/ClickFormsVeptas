     minof3 =gComp1
  End If
  If (gComp2 < gComp1) and (gComp2 < gComp3) Then
     minof3 = gComp2
  End If
  If (gComp3 < gComp1) and (gComp3 < gComp2) Then
    minof3 = gComp3
  End If
end Function

