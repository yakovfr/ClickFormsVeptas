'/////////////////////////  Functions to test for signature date, effective date and expiration date ///////////////////////
' Show warning if signature date not = today's date
Sub IsSignatureDateNOTToday(APage, aCell)
 IDSignatureDateNOTToDay = "Signature Date not Today's Date."
 If hasText(APage, ACell) then
   sigDate  = GetText(APage, ACell)
   If not IsDate(sigDate) then
     AddRec "Invalid Signature Date: "&SigDate, aPage, aCell
   End If
   If isDate(sigDate) then
     If (DateDiff("d",sigDate,Date) <> 0) then
       AddRec IDSignatureDateNOTToDay, APage, ACell
     End If
   End If
  End If
End Sub

'Show warning if effective date already past signature date for more than 2 weeks
Sub IsEffectiveDatePastSignatureDate2wks(sPage, sCell, ePage, eCell)
Dim DatesOK
  IDEffectiveDatePast2Wks = "Effective Date of Appraisal is more than 14 days ago."
  If hasText(sPage, sCell) and hasText(ePage,eCell)  then
    sigDate = GetText(sPage,sCell)
    effDate = GetText(ePage, eCell)
    DatesOK = True
    If not isDate(sigDate) then
      AddRec "Invalid Signature Date: "&sigDate, sPage, sCell
      DatesOK = False
    End If
    If not isDate(effDate) then
      AddRec "Invalid Effective Date: "&effDate, ePage, eCell
      DatesOK = False
    End If
    If DatesOK then
      If (DateDiff("d", effDate, sigDate) > 14)  then
        AddRec IDEffectiveDatePast2Wks, ePage, eCell
      End If
    End If
  End If
End Sub

