'Show warning if Settled Date is BEFORE Contract Date
' Format: smm/yy;cmm/yy  where s: settle date, c: contract date mm/dd is month/day
Sub IsSettledDateBEFOREContractDate(sPage, sCell)
  IDSettleDateBEFOREContractDate = "Settle Date is BEFORE Contract Date."
  sDate = GetText(sPage,sCell)
  s = Mid(sDate,1,1)
  sMonth = Mid(sDate,2,2)
  sYear = Mid(sDate,5,2)

  c = Mid(sDate,8,1)
  cMonth = Mid(sDate,9,2)
  cYear = Mid(sDate,12,2)
  sYearB4 = (sYear < cYear)
  sYearEQ = (sYear = cYear)
  'only compare if it's "s" and "c"
  If (s = "s") and (c = "c") then  'check for settle year is before or the same as contract year
    If sYearB4 then
      AddRec IDSettleDateBEFOREContractDate, sPage,sCell
    Else
      If sYearEQ and (sMonth < cMonth) then
        AddRec IDSettleDateBEFOREContractDate, sPage,sCell
      End If
    End If
  End If
End Sub


