  Else
    GetCellUADError = False
  End If  
end Function

Function FormCompNumErrStr(CompStr, APageID, ASequenceID)
Dim WarnStr, PropLen
   If ASequenceID > 0 then
     If Mid(CompStr, 1, 2) = "**" then
       ErrFlag = "**"
       WarnStr = Mid(CompStr, 3, Len(CompStr))
     Else
       ErrFlag = ""
       WarnStr = CompStr
     End If
     If Mid(WarnStr, 1, 4) = "LIST" Then
       PropLen = 8
     Else
       PropLen = 5
     End If
     If APageID > 0 then
       FormCompNumErrStr = ErrFlag & Mid(WarnStr, 1, PropLen) & GetText(APageID, ASequenceID) & Mid(WarnStr, (PropLen+2), Len(WarnStr))
     Else
       FormCompNumErrStr = ErrFlag & Mid(WarnStr, 1, PropLen) & ASequenceID & Mid(WarnStr, (PropLen+2), Len(WarnStr))
     End If
   Else
     FormCompNumErrStr = CompStr
   End If
End Function

' === Function to return the min value of the 3 numbers ===
Function  minof3(gComp1,gComp2,gComp3)
  If (gComp1 < gComp2) and (gComp1 < gComp3) Then
     minof3 =gComp1
  End If
  If (gComp2 < gComp1) and (gComp2 < gComp3) Then
     minof3 = gComp2
  End If
  If (gComp3 < gComp1) and (gComp3 < gComp2) Then
    minof3 = gComp3
  End If
end Function

' === Function to return the max value of the 3 numbers ===
Function maxof3(gComp1,gComp2,gComp3)
  If (gComp1 > gComp2) and (gComp1 > gComp3) Then
    maxof3 =gComp1
  End If
  If (gComp2 > gComp1) and (gComp2 > gComp3) Then
    maxof3 = gComp2
  End If
  If (gComp3 > gComp1) and (gComp3 > gComp2) Then
    maxof3 = gComp3
  End If
end Function

