Function Require(AnErrorMessageCode, APageID, ACellID)
   If not hasText(APageID, ACellID) Then
       If AnErrorMessageCode = "" Then
           AnErrorMessageCode = "Required value " & REQUIRED_DATA_MISSING
       End If
       AddRec AnErrorMessageCode, APageID, ACellID
       Require = True
   Else
       Require = False
   End If
End Function

