Function CheckAndComment(APageID, CheckID, CommentID, CheckWithoutCommentMessageID, CommentWithoutCheckMessageID)
Dim ThisCommentText

   CheckAndComment = True

   If CheckWithoutCommentMessageID = "" Then
       CheckWithoutCommentMessageID = CHECK_NO_COMMENT
   End If

   If CheckWithoutCommentMessageID = "" Then
       CommentWithoutCheckMessageID = COMMENT_NO_CHECK
   End If

   If isChecked(APageID, CheckID) and (not hasText(APageID, CommentID)) Then
       AddRec CheckWithoutCommentMessageID, APageID, CommentID
   Else
       If (not isChecked(APageID, CheckID)) and hasText(APageID, CommentID) Then
           If not isNegativeComment(APageID, CommentID) Then
               AddRec CommentWithoutCheckMessageID, APageID, CheckID
           End If
       Else
           CheckAndComment = False
       End If
   End If
End Function

Private Function BaseValueBetween(AValue, AValuePageID, AValueID, ACheckPageID, ALowValueID, AHighValueID, TooLowMessageID, TooHighMessageID)
Dim ThisValue

   BaseValueBetween = True

   If AValue < GetValue(ACheckPageID, ALowValueID) Then
       If TooLowMessageID = "" Then
           TooLowMessageID = "Value of " & AValue & " is below the bottom limit of " & GetValue(ACheckPageID, ALowValueID)
       End If
       AddRec TooLowMessageID, AValuePageID, AValueID
   Else
       If AValue > GetValue(ACheckPageID, AHighValueID) Then
           If TooHighMessageID = "" Then
               TooHighMessageID = "Value of " & AValue & " is above the top limit of " & GetValue(ACheckPageID, AHighValueID)
           End If
           AddRec TooHighMessageID, AValuePageID, AValueID
       Else
           BaseValueBetween = False
       End If
   End If
End Function

Function ValueInThousandsBetween(AValuePageID, AValueID, ACheckPageID, ALowValueID, AHighValueID, TooLowMessageID, TooHighMessageID)

   ValueInThousandsBetween = BaseValueBetween(GetValue(AValuePageID, AValueID) / 1000, AValuePageID, AValueID, ACheckPageID, ALowValueID, AHighValueID, TooLowMessageID, TooHighMessageID)

End Function

Function ValueBetween(AValuePageID, AValueID, ACheckPageID, ALowValueID, AHighValueID, TooLowMessageID, TooHighMessageID)

   ValueBetween = BaseValueBetween(GetValue(AValuePageID, AValueID), AValuePageID, AValueID, ACheckPageID, ALowValueID, AHighValueID, TooLowMessageID, TooHighMessageID)

End Function

Function ValuesMustMatch(APage1ID, AValue1ID, APage2ID, AValue2ID, NonMatchMessageID)

   If strComp(GetText(APage1ID, AValue1ID), GetText(APage2ID, AValue2ID)) = 0 Then
       ValuesMustMatch = False
   Else
       ValuesMustMatch = True
       AddRec NonMatchMessageID, APage1ID, AValue1ID
   End If
End Function

Sub ValidateStandardSignatureBlock(APageID, AnAppraiserNameID)

   Require IDDATESIGNED, APageID, AnAppraiserNameID + 6
   Count = GetTextCheck(APageID, AnAppraiserNameID + 8) + GetTextCheck(APageID, AnAppraiserNameID + 9) + GetTextCheck(APageID, AnAppraiserNameID + 10)
   If Count = 0 Then AddRec IDNOCERT, APageID, AnAppraiserNameID + 8
   If Count > 1 Then AddRec IDCERT, APageID, AnAppraiserNameID + 8

   If hasText(APageID, AnAppraiserNameID + 8) and (not hasText(APageID, AnAppraiserNameID + 12)) Then AddRec IDCERTSTATE, APageID, AnAppraiserNameID + 12
   If (hasText(APageID, AnAppraiserNameID + 9) or hasText(APageID, AnAppraiserNameID + 10)) and (not hasText(APageID, AnAppraiserNameID + 12)) Then AddRec IDLICSTATE, APageID, AnAppraiserNameID + 12
   If (Count >= 1) and (not hasText(APageID, AnAppraiserNameID + 13)) Then AddRec IDLICEXPIRE, APageID, AnAppraiserNameID + 13

   If hasText(APageID, AnAppraiserNameID + 10) and (not hasText(APageID, AnAppraiserNameID + 11)) Then AddRec IDLICOTHER, APageID, AnAppraiserNameID + 11
   If (not hasText(APageID, AnAppraiserNameID + 10)) and hasText(APageID, AnAppraiserNameID + 11) Then AddRec IDLICOTHER, APageID, AnAppraiserNameID + 10

   Count = GetTextCheck(APageID, AnAppraiserNameID + 29) + GetTextCheck(APageID, AnAppraiserNameID + 30)
   If Count = 0 Then AddRec IDNOCERT, APageID, AnAppraiserNameID + 29
   If Count > 1 Then AddRec IDCERT, APageID, AnAppraiserNameID + 29

   If hasText(APageID, AnAppraiserNameID + 29) and (not hasText(APageID, AnAppraiserNameID + 31)) Then AddRec IDCERTSTATE, APageID, AnAppraiserNameID + 31
   If hasText(APageID, AnAppraiserNameID + 30) and (not hasText(APageID, AnAppraiserNameID + 31)) Then AddRec IDLICSTATE, APageID, AnAppraiserNameID + 31
   If (hasText(APageID, AnAppraiserNameID + 29) or hasText(APageID, AnAppraiserNameID + 30)) and (not hasText(APageID, AnAppraiserNameID + 32)) Then AddRec IDLICEXPIRE, APageID, AnAppraiserNameID + 32

   OnlyOneCheckOfThree APageID, AnAppraiserNameID + 33, AnAppraiserNameID + 34, AnAppraiserNameID + 3, IDSUPINSPECTSUBJNOCHK, IDSUPINSPECTSUBJCHK
   CheckAndComment APageID, AnAppraiserNameID + 34, AnAppraiserNameID + 35, IDCHECKNOCOMMENT, IDCOMMENTNOCHECK
   CheckAndComment APageID, AnAppraiserNameID + 3, AnAppraiserNameID + 37, IDCHECKNOCOMMENT, IDCOMMENTNOCHECK

   OnlyOneCheckOfTwo APageID, AnAppraiserNameID + 38, AnAppraiserNameID + 39, IDSUPINSPECTCOMPNOCHK, IDSUPINSPECTCOMPCHK
   CheckAndComment APageID, AnAppraiserNameID + 39, AnAppraiserNameID + 49, IDCHECKNOCOMMENT, IDCOMMENTNOCHECK
End Sub

