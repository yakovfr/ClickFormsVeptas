Sub ValidateStandardNeighborhoodBlock(APageID, AnUrbanID)      '       e.g. 52
Dim AllocatedLandPercent

   OnlyOneCheckOfThree APageID, AnUrbanID, AnUrbanID + 1, AnUrbanID + 2, IDLocNoChk, IDLocChk

   OnlyOneCheckOfThree APageID, AnUrbanID + 3, AnUrbanID + 4, AnUrbanID + 5, IDBuiltNoChk, IDBuiltChk

   AllocatedLandPercent = GetValue(APageID, AnUrbanID + 24) + GetValue(APageID, AnUrbanID + 25)+ GetValue(APageID, AnUrbanID + 26) + GetValue(APageID, AnUrbanID + 27) + GetValue(APageID, AnUrbanID + 28)
   If AllocatedLandPercent <> 100 Then AddRec IDLandUse, APageID, AnUrbanID + 24

   If isChecked(APageID, AnUrbanID + 2) Then      '   BuiltUp over 75%
       If (AllocatedLandPercent < 75) Then AddRec IDOver75, APageID, AnUrbanID + 4
   End If

   If isChecked(APageID, AnUrbanID + 4) Then                '           BuiltUp between 25%and 75%
       If (AllocatedLandPercent >= 75) or (AllocatedLandPercent <= 25) Then AddRec ID2575, APageID, AnUrbanID + 4
   End If

   If isChecked(APageID, AnUrbanID + 5) Then
       If (AllocatedLandPercent > 25) Then AddRec IDUNDER25, APageID, AnUrbanID + 5
   End If

   OnlyOneCheckOfThree APageID, AnUrbanID + 6, AnUrbanID + 7, AnUrbanID + 8, IDGrowthNoChk, IDGrowthChk

   OnlyOneCheckOfThree APageID, AnUrbanID + 9, AnUrbanID + 10, AnUrbanID + 11, IDPrpValNoChk, IDPrpValChk

   OnlyOneCheckOfThree APageID, AnUrbanID + 12, AnUrbanID + 13, AnUrbanID + 14, IDDemandNoChk, IDDemandChk

   OnlyOneCheckOfThree APageID, AnUrbanID + 15, AnUrbanID + 16, AnUrbanID + 17, IDMarketNoChk, IDMarketChk

   If isChecked(APageID, AnUrbanID + 17) and (isChecked(APageID, AnUrbanID + 9) or isChecked(APageID, AnUrbanID + 12)) Then AddRec IDMarketOver, APageID, AnUrbanID + 17
   If isChecked(APageID, AnUrbanID + 15) and (isChecked(APageID, AnUrbanID + 11) or isChecked(APageID, AnUrbanID + 14)) Then AddRec IDMarketUnder, APageID, AnUrbanID + 15

   nChecks = GetCheck(APageID, AnUrbanID + 2) + GetCheck(APageID, AnUrbanID + 5) + GetCheck(APageID, AnUrbanID + 8) + GetCheck(APageID, AnUrbanID + 11) + GetCheck(APageID, AnUrbanID + 14) + Getcheck(APageID, AnUrbanID + 17)
   If nChecks > 0 Then AddRec IDRComment, APageID, AnUrbanID + 2


'  No Land Use Change


'  No Predominant Occupancy

   ' No Vacancy

   Require IDLowPrice, APageID, AnUrbanID + 18
   Require IDHighPrice, APageID, AnUrbanID + 19
   If hasText(APageID, AnUrbanID + 18) and hasText(APageID, AnUrbanID + 19) and (GetValue(APageID, AnUrbanID + 19) < GetValue(APageID, AnUrbanID + 18)) Then AddRec IDPriceCK, APageID, AnUrbanID + 18
   If not Require(IDPredPrice, APageID, AnUrbanID + 20) Then ValueBetween APageID, AnUrbanID + 20, APageID, AnUrbanID + 18, AnUrbanID + 19, IDPriceRange, IDPriceRange

   Require IDLowAge, APageID, AnUrbanID + 21
   Require IDHighAge, APageID, AnUrbanID + 22
   If hasText(APageID, AnUrbanID + 21) and hasText(APageID, AnUrbanID + 22) and (GetValue(APageID, AnUrbanID + 22) < GetValue(APageID, AnUrbanID + 21)) Then AddRec IDAgeCK, APageID, AnUrbanID + 21
   If not Require(IDPredAge, APageID, AnUrbanID + 23) Then ValueBetween APageID, AnUrbanID + 23, APageID, AnUrbanID + 21, AnUrbanID + 22, IDAgeRange, IDAgeRange

   Require "", APageID, AnUrbanID + 29     '   Neighborhood Boundaries
End Sub



