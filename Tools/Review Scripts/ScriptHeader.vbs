'valid format: dd/mm/yyyy
Function IsDateFormatOK(pg, cl)
	dateStr = GetText(pg,cl)
	if dateStr = "" then
		bOK = true
	else
		bOK = isDate(dateStr)
		if bOK then
			dt = CDate(dateStr)
			d = day(dt)
			if d < 10 then
				dStr = "0" & d
			else
				dStr = d
			end if
			m = month(dt)
			if m < 10 then 
				mStr = "0" & m
			else
				mStr = m
			end if
			validStr = mStr & "/" & dStr & "/" & year(dt)
			cmp = strComp(dateStr, validStr)
			if cmp <> 0 then
				bOK = false
			end if
		end if
	end if
	IsDateFormatOK = bOK
end function

Function isNegativeComment(APageID, ASequenceID)
Dim CommentText

   CommentText = LCase(GetText(APageID, ASequenceID))
   If (CommentText = "none") or (CommentText = "no") or (CommentText = "na") or (CommentText = "n/a") Then
       isNegativeComment = True
   Else
       isNegativeComment = False
   End If

   if isNumeric(CommentText) then isNegativeComment = False

End Function

Function IsEnergyEfficientOK(pg, cl)
Dim InvalidList, Cntr, ItemStr

  IsEnergyEfficientOK = True
  If IsUAD Then
    If hasText(pg, cl) Then
      ItemStr = UCase(GetText(pg, cl))
      InvalidList = Array("AVERAGE","TYPICAL","STANDARD","N/A","NOT AVAILABLE","NOT APPLICABLE")
      For Cntr = 0 to 5
        If ItemStr = InvalidList(Cntr) Then
          IsEnergyEfficientOK = False
        End If
      Next
    End If
  End If
end Function

Function GetCellUADError(pg, cl)
  If IsUAD then
